@article{lombard_reliability_2017,
	title = {Reliability of metrics associated with a counter-movement jump performed on a force plate},
	volume = {21},
	issn = {1091367X},
	url = {https://ezproxy.massey.ac.nz/login?url=https://search.ebscohost.com/login.aspx?direct=true&AuthType=sso&db=aph&AN=125829464&site=eds-live&scope=site&authtype=sso&custid=s3027306},
	doi = {10.1080/1091367X.2017.1354215},
	abstract = {The counter-movement jump is a consequence of maximal force, rate of force developed, and neuromuscular coordination. Thus, the counter-movement jump has been used to monitor various training adaptations. However, the smallest detectable difference of counter-movement jump metrics has yet to be established. The objective of the present study was to measure the reliability of counter-movement jump metrics, including rate of force development, flight time, time to max force, and max force. Twenty-nine male participants (mean age 25 ± 3 years) were divided into three groups. Each participant performed five counter-movement jumps on a force plate, on three consecutive days. Flight time detected trivial changes, (effect size {\textless} .2) and typical error of measurement of .25\%; max force detected small changes (effect size {\textless} .5) with a typical error of measurement of .3\%; rate of force development detected small to medium change (effect size .5–.8) with a typical error of measurement of .3\%.},
	pages = {235--243},
	number = {4},
	journaltitle = {Measurement in Physical Education \& Exercise Science},
	author = {Lombard, Wayne and Reid, Sorrel and Pearson, Keagan and Lambert, Michael},
	urldate = {2024-08-24},
	date = {2017-10},
	note = {Publisher: Taylor \& Francis Ltd},
	keywords = {rate of force development, {JUMPING}, flight time, max force, {MEDICAL} protocols, {RELIABILITY} (Personality trait)},
