}

@article{ercan_acute_2017,
	title = {The Acute Effect of Static and Dynamic Stretching on Horizontal and Vertical Jump},
	volume = {7},
	abstract = {Stretching is an exercise recommended by health professionals and commonly used by athletes for warming before the performance and injury prevention. The aim of this study is to investigate acute effects of dynamic and static stretching on horizontal-vertical jump distance of recreational athletes and determine which stretching type should be used in sports requiring high power and anaerobic performance such as jumping by revealing how stretching type affects horizontal-vertical jump performance. 47 males with a mean (±Standard Deviation) age of 20.6 (±1.7) years, a mean height of 178.2 (±5.9) cm, a mean body weight of 72.4 (±9.1) kg, and a mean body mass index of 22.8 (±2.7) kg/m 2 were included in this study. All participants were applied three different programs involving only warm-up (method A), warm-up+ dynamic stretching (method B) and warm-up+ static stretching (method C) after 5 minutes of warm-up run and 2 minutes of active rest. After warm-up and stretching protocols, horizontal and vertical jump tests were performed 3 times and the best values were used for analysis. " The descriptive data analysis method " , " the repeated measures {ANOVA} test " and " the {LSD} test " were performed using {SPSS} 20.0 for data analysis. The results are presented as mean± standard deviation. p{\textless}0.05 was accepted as the statistical significance level. According to the data obtained in the study, different stretching types did not affect horizontal jump distance (p: 0.44, F: 0.82), whereas a statistically significant difference was found in terms of vertical jump distance after static stretching (p: 0.048, F: 2.86). As a result, we can say that both dynamic and static stretching could be safely used before sport activities requiring high power and static stretching leads to positive results on a statistically significant level.},
	pages = {2251--7502},
	journaltitle = {International Journal of Sport Studies},
	shortjournal = {International Journal of Sport Studies},
	author = {Ercan, Sabriye and Başkurt, Zeliha and Başkurt, Ferdi and Ince Parpucu, Tuba},
	date = {2017-08-01},
	file = {Full Text PDF:C\:\\Users\\David\\Zotero\\storage\\VIJ28GYX\\Ercan et al. - 2017 - The Acute Effect of Static and Dynamic Stretching .pdf:application/pdf},
}

@article{arntz_chronic_2023,
	title = {Chronic Effects of Static Stretching Exercises on Muscle Strength and Power in Healthy Individuals Across the Lifespan: A Systematic Review with Multi-level Meta-analysis},
	volume = {53},
	issn = {0112-1642, 1179-2035},
	url = {https://link.springer.com/10.1007/s40279-022-01806-9},
	doi = {10.1007/s40279-022-01806-9},
	shorttitle = {Chronic Effects of Static Stretching Exercises on Muscle Strength and Power in Healthy Individuals Across the Lifespan},
	abstract = {Background  The current literature on the chronic effects of static stretching ({SS}) exercises on muscle strength and power is unclear and controversial.
Objective  We aimed to examine the chronic effects of {SS} exercises on muscle strength and power as well as flexibility in healthy individuals across the lifespan. Design  Systematic review with meta-analysis of (randomized) controlled trials. Data Sources  A systematic literature search was conducted in the databases {PubMed}, Web of Science, Cochrane Library, and {SPORTDiscus} up to May 2022. Eligibility Criteria for Selecting Studies  We included studies that investigated the chronic effects of {SS} exercises on at least one muscle strength and power outcome compared to an active/passive control group or the contralateral leg (i.e., using between- or within-study designs, respectively) in healthy individuals, irrespective of age, sex, and training status.
Results  The main findings of 41 studies indicated trivial-to-small positive effects of chronic {SS} exercises on muscle strength (standardized mean difference [{SMD}] = 0.21, [95\% confidence interval 0.10–0.32], p = 0.001) and power ({SMD} = 0.19, 95\% confidence interval 0.12–0.26], p {\textless} 0.001). For flexibility, moderate-to-large increases were observed ({SMD} = 0.96, [95\% confidence interval 0.70–1.22], p {\textless} 0.001). Subgroup analyses, taking the participants’ training status into account, revealed a larger muscle strength improvement for sedentary ({SMD} = 0.58, p {\textless} 0.001) compared with recreationally active participants ({SMD} = 0.16, p = 0.029). Additionally, larger flexibility gains were observed following passive ({SMD} = 0.97, p {\textless} 0.001) compared with active {SS} exercises ({SMD} = 0.59, p = 0.001). The chronic effects of {SS} on muscle strength were moderated by the proportion of female individuals in the sample (β = 0.004, p = 0.042), with higher proportions experiencing larger gains. Other moderating variables included mean age (β = 0.011, p {\textless} 0.001), with older individuals showing larger muscle strength gains, and the number of repetitions per stretching exercise and session (β = 0.023, p = 0.004 and β = 0.013, p = 0.008, respectively), with more repetitions associated with larger muscle strength improvements. Muscle power was also moderated by mean age (β = 0.006, p = 0.007) with larger gains in older individuals. The meta-regression analysis indicated larger flexibility gains with more repetitions per session (β = 0.094, p = 0.016), more time under stretching per session (β = 0.090, p = 0.026), and more total time under stretching (β = 0.078, p = 0.034).
Conclusions  The main findings indicated that chronic {SS} exercises have the potential to improve muscle strength and power. Such improvements appear to benefit sedentary more than recreationally active participants. Likewise, chronic {SS} exercises result in a marked enhancement in flexibility with larger effects of passive, as compared with active, {SS}. The results of the meta-regression analysis for muscle strength indicated larger benefits of chronic {SS} exercises in samples with higher proportions of female, older participants, and a higher number of repetitions per stretching exercise and session. For muscle power, results suggested larger gains for older participants. Regarding flexibility, findings indicated larger benefits following a higher number of repetitions per exercise and a longer time under stretching per session as well as a longer total time under stretching.},
	pages = {723--745},
	number = {3},
	journaltitle = {Sports Medicine},
	shortjournal = {Sports Med},
	author = {Arntz, Fabian and Markov, Adrian and Behm, David G. and Behrens, Martin and Negra, Yassine and Nakamura, Masatoshi and Moran, Jason and Chaabene, Helmi},
	urldate = {2024-07-23},
	date = {2023-03},
	langid = {english},
	file = {Arntz et al. - 2023 - Chronic Effects of Static Stretching Exercises on .pdf:C\:\\Users\\David\\Zotero\\storage\\TLJZ3JBR\\Arntz et al. - 2023 - Chronic Effects of Static Stretching Exercises on .pdf:application/pdf},
}

@article{taylor_negative_2009,
	title = {Negative effect of static stretching restored when combined with a sport specific warm-up component},
	volume = {12},
	rights = {https://www.elsevier.com/tdm/userlicense/1.0/},
	issn = {14402440},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S1440244008000790},
	doi = {10.1016/j.jsams.2008.04.004},
	abstract = {There is substantial evidence that static stretching may inhibit performance in strength and power activities. However, most of this research has involved stretching routines dissimilar to those practiced by athletes. The purpose of this study was to evaluate whether the decline in performance normally associated with static stretching pervades when the static stretching is conducted prior to a sport speciﬁc warm-up. Thirteen netball players completed two experimental warm-up conditions. Day 1 warm-up involved a submaximal run followed by 15 min of static stretching and a netball speciﬁc skill warm-up. Day 2 followed the same design; however, the static stretching was replaced with a 15 min dynamic warm-up routine to allow for a direct comparison between the static stretching and dynamic warm-up effects. Participants performed a countermovement vertical jump and 20 m sprint after the ﬁrst warm-up intervention (static or dynamic) and also after the netball speciﬁc skill warm-up. The static stretching condition resulted in signiﬁcantly worse performance than the dynamic warm-up in vertical jump height (−4.2\%, 0.40 {ES}) and 20 m sprint time (1.4\%, 0.34 {ES}) (p {\textless} 0.05). However, no signiﬁcant differences in either performance variable were evident when the skill-based warm-up was preceded by static stretching or a dynamic warm-up routine. This suggests that the practice of a subsequent high-intensity skill based warm-up restored the differences between the two warm-up interventions. Hence, if static stretching is to be included in the warm-up period, it is recommended that a period of high-intensity sport-speciﬁc skills based activity is included prior to the on-court/ﬁeld performance.},
	pages = {657--661},
	number = {6},
	journaltitle = {Journal of Science and Medicine in Sport},
	shortjournal = {Journal of Science and Medicine in Sport},
	author = {Taylor, Kristie-Lee and Sheppard, Jeremy M. and Lee, Hamilton and Plummer, Norma},
	urldate = {2024-07-23},
	date = {2009-11},
	langid = {english},
	file = {Taylor et al. - 2009 - Negative effect of static stretching restored when.pdf:C\:\\Users\\David\\Zotero\\storage\\Q6ILACKC\\Taylor et al. - 2009 - Negative effect of static stretching restored when.pdf:application/pdf},
}

@article{brandenburg_time_2007,
	title = {Time Course of Changes in Vertical-Jumping Ability After Static Stretching},
	volume = {2},
	doi = {10.1123/ijspp.2.2.170},
	abstract = {To examine the acute effects of static stretching on countermovement vertical-jump ({CMVJ}) ability and monitor the time course of any stretch-induced changes.
Once familiarized, 16 experienced jumpers completed 2 testing sessions in a randomized order. Each session consisted of a general warm-up, a pretreatment {CMVJ} assessment, a treatment, and multiple posttreatment {CMVJ} assessments. One treatment included lower-body static stretching, and the second treatment, involving no stretching, was the control. Posttreatment {CMVJ} measures occurred immediately, 3, 6, 12, and 24 minutes posttreatment. Stretching consisted of 3 static-stretching exercises, with each exercise repeated 3 times and each repetition held for 30 s.
Prestretch {CMVJ} height equaled 47.1 (+/- 9.7) cm. {CMVJ} height immediately poststretch was 45.7 (+/- 9.2) cm, and it remained depressed during the 24-min follow-up period. Pre-no-stretch {CMVJ} height was 48.4 (+/- 9.8) cm, whereas immediately post-no-stretch {CMVJ} height equaled 46.8 (+/- 9.5) cm, and as in the stretch treatment, post-no-stretch {CMVJ} height remained lower than pre-no-stretch values.Although there was a significant main effect of time (P = .005), indicating that {CMVJ} was lower and remained impaired after both treatments, no significant interaction effect (P = .749) was observed.
In comparison with the no-activity control, static stretching resulted in similar reductions in {CMVJ} ability when examined over the same time course, so athletes preparing for {CMVJ} should avoid periods of inactivity, as well as static stretching.},
