@article{beerRelativeSeverityHuman2024,
  title = {Relative {{Severity}} of {{Human Performance Decrements Recorded}} in {{Rapid}} vs. {{Gradual Decompression}}},
  author = {Beer, Jeremy and Mojica, Andrew J. and Blacker, Kara J. and Dart, Todd S. and Morse, Bria G. and Sherman, Paul M.},
  date = {2024-07-01},
  journaltitle = {Aerospace Medicine and Human Performance},
  shortjournal = {aerosp med hum perform},
