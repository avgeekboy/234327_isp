  pages = {S40.007},
  publisher = {Wolters Kluwer},
  doi = {10.1212/WNL.82.10_supplement.S40.007},
  url = {https://www.neurology.org/doi/abs/10.1212/WNL.82.10_supplement.S40.007},
  urldate = {2024-06-12},
  abstract = {Introduction: An increase in white matter hyperintensities (WMH) on high resolution fluid attenuated inversion recovery (FLAIR) MRI imaging has been demonstrated in United States Air Force high-altitude U-2 pilots (U2P) compared to matched normative controls (DOC), believed secondary to hypobaric non-hypoxic exposure. Altitude chamber physiology technicians (PHY) exposed to shorter periods of hypobaric non-hypoxic conditions has not been reported. Our hypothesis was the exposure experienced by PHY would demonstrate similar increased subcortical WMH on FLAIR imaging compared to a comparable DOC population but with no significant difference in findings between U2P and PHY. Objective: To compare a population of similar age, medical and neurological history non-exposed controls (DOC) to subjects with a history of hypobaric non-hypoxic exposure (U2P; PHY). Design/Methods: We examined 105 U2P and 83 PHY and compared these to 132 DOC. Quantitative measurement of subcortical FLAIR lesion count and volume was performed as previously described. We applied the 2-tailed nonparametric Mann Whitney statistical test, considering p{$<$}0.05 significant. Results: U2P, PHY, and DOC (respectively): subcortical WMH number: 9.67±18.26; 6.37±11.12; 2.83±3.38; subcortical WMH volume: 0.147cm3±0.296; 0.126cm3±0.404; 0.036cm3±0.061. Significance for subcortical WMH number and volume (respectively): U2P vs. DOC p{$<$}0.001 and p{$<$}0.001; PHY vs. DOC p=0.044 and p=0.033; U2P vs. PHY p=0.091 and p=0.237. Conclusions: The total number and volume of subcortical WMH found in U2P and PHY is significantly greater than in DOC with no significant difference between U2P and PHY. This study provides further evidence that exposure to hypobaria without hypoxia leads to white matter change even in the absence of clinical symptoms. Research is ongoing examining the pathophysiology underlying these structural subcortical changes detected on MRI. Study supported by: AF Surgeon General grant (Log I-11-44). Disclosure: Dr. Robinson has nothing to disclose. Dr. Grogan has received personal compensation for activities with US Worldmeds as a speaker. Dr. Kochunov has nothing to disclose. Dr. Sherman has nothing to disclose. Dr. McGuire has nothing to disclose.},
  issue = {10\_supplement}
}

@article{rocha-rodriguesSkeletalMuscleAdipose2021,
  title = {Skeletal Muscle–Adipose Tissue–Tumor Axis: {{Molecular}} Mechanisms Linking Exercise Training in Prostate Cancer},
  author = {Rocha-Rodrigues, S. and Matos, A. and Afonso, J. and Mendes-Ferreira, M. and Abade, E. and Teixeira, E. and Silva, B. and Murawska-Ciałowicz, E. and Oliveira, M. J. and Ribeiro, R.},
  date = {2021},
  journaltitle = {International Journal of Molecular Sciences},
  volume = {22},
  number = {9},
  doi = {10.3390/ijms22094469},
  url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85104533166&doi=10.3390%2fijms22094469&partnerID=40&md5=13665640ed75dd466ceb55036aec1e94 https://mdpi-res.com/d_attachment/ijms/ijms-22-04469/article_deploy/ijms-22-04469.pdf?version=1619324900},
  abstract = {Increased visceral adiposity may influence the development of prostate cancer (PCa) aggressive tumors and cancer mortality. White adipose tissue (WAT), usually referred to as periprostatic adipose tissue (PPAT), surrounds the prostatic gland and has emerged as a potential mediator of the tumor microenvironment. Exercise training (ET) induces several adaptations in both skeletal muscle and WAT. Some of these effects are mediated by ET-induced synthesis and secretion of several proteins, known as myo-and adipokines. Together, myokines and adipokines may act in an endocrine-like manner to favor communication between skeletal muscle and WAT, as they may work together to improve whole-body metabolic health. This crosstalk may constitute a potential mechanism by which ET exerts its beneficial role in the prevention and treatment of PCa-related disorders; however, this has not yet been explored. Therefore, we reviewed the current evidence on the effects of skeletal muscle–WAT–tumor crosstalk in PCa, and the potential mediators of this process to provide a better understanding of underlying ET-related mechanisms in cancer. © 2021 by the authors. Licensee MDPI, Basel, Switzerland.},
  keywords = {Cancer,Periprostatic fat,Physical activity,Skeletal muscle,Tumor microenvironment,Visceral adiposity},
  file = {/home/david/Zotero/storage/UE99AMY7/Rocha-Rodrigues-2021-Skeletal muscle–adipose t.pdf}
}

