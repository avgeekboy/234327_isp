@article{morettiniEffectsWalkingLowgrade2015,
  title = {Effects of Walking on Low-Grade Inflammation and Their Implications for {{Type}} 2 {{Diabetes}}},
  author = {Morettini, M. and Storm, F. and Sacchetti, M. and Cappozzo, A. and Mazzà, C.},
  date = {2015},
  journaltitle = {Preventive Medicine Reports},
