  volume = {7},
  pages = {2251--7502},
  abstract = {Stretching is an exercise recommended by health professionals and commonly used by athletes for warming before the performance and injury prevention. The aim of this study is to investigate acute effects of dynamic and static stretching on horizontal-vertical jump distance of recreational athletes and determine which stretching type should be used in sports requiring high power and anaerobic performance such as jumping by revealing how stretching type affects horizontal-vertical jump performance. 47 males with a mean (±Standard Deviation) age of 20.6 (±1.7) years, a mean height of 178.2 (±5.9) cm, a mean body weight of 72.4 (±9.1) kg, and a mean body mass index of 22.8 (±2.7) kg/m 2 were included in this study. All participants were applied three different programs involving only warm-up (method A), warm-up+ dynamic stretching (method B) and warm-up+ static stretching (method C) after 5 minutes of warm-up run and 2 minutes of active rest. After warm-up and stretching protocols, horizontal and vertical jump tests were performed 3 times and the best values were used for analysis. " The descriptive data analysis method " , " the repeated measures ANOVA test " and " the LSD test " were performed using SPSS 20.0 for data analysis. The results are presented as mean± standard deviation. p{$<$}0.05 was accepted as the statistical significance level. According to the data obtained in the study, different stretching types did not affect horizontal jump distance (p: 0.44, F: 0.82), whereas a statistically significant difference was found in terms of vertical jump distance after static stretching (p: 0.048, F: 2.86). As a result, we can say that both dynamic and static stretching could be safely used before sport activities requiring high power and static stretching leads to positive results on a statistically significant level.},
  file = {/home/david/Zotero/storage/VIJ28GYX/Ercan et al. - 2017 - The Acute Effect of Static and Dynamic Stretching .pdf}
}

@article{ertekImpactPhysicalActivity2012,
  title = {Impact of Physical Activity on Inflammation: {{Effects}} on Cardiovascular Disease Risk and Other Inflammatory Conditions},
  author = {Ertek, S. and Cicero, A.},
  date = {2012},
  journaltitle = {Archives of Medical Science},
