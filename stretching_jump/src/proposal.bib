@article{wilkersonSportFitnessIndex2016,
  title = {A {{Sport Fitness Index}} for {{Assessment}} of {{Sport-Related Injury Risk}}},
  author = {Wilkerson, Gary B. and Colston, Marisa A. and Baker, Carrie S.},
  date = {2016-09},
  journaltitle = {Clinical Journal of Sport Medicine},
  volume = {26},
  number = {5},
  pages = {423},
  issn = {1050-642X},
  doi = {10.1097/JSM.0000000000000280},
  url = {https://journals.lww.com/cjsportsmed/abstract/2016/09000/a_sport_fitness_index_for_assessment_of.12.aspx},
  urldate = {2024-06-21},
  abstract = {Objective:~           To develop and validate a concise survey that will identify athletes who possess elevated injury risk.           Design:~           Cohort study.           Setting:~           National Collegiate Athletic Association Division I athletic program.           Participants:~           Cohorts of 188 and 146 college athletes who participated during successive academic years.           Assessment of Risk Factors:~           The first cohort provided responses to 4 joint-specific outcome surveys that were related to subsequent time-loss injury occurrence. Receiver operating characteristic analysis identified survey items that provided best discrimination, which were combined with a negative life event stress item to create a new 10-item survey. The second cohort provided responses to the new survey, which were converted to a 0 to 100 score.           Main Outcome Measures:~           Construct validity was assessed through documentation of time-loss injuries sustained during the preceding 12 months and predictive validity was assessed through prospective documentation of sport-related sprains and strains. Cronbach alpha was calculated to assess internal consistency.           Results:~           Each of the outcome survey items used to develop the new survey demonstrated much greater specificity than sensitivity. Both the retrospective and prospective receiver operating characteristic (ROC) analyses identified scores of 96 and 88 as cut-points that provided good discrimination between injured and noninjured cases. The area under the curve was 0.69 (P {$<$} 0.001) for the retrospective analysis and 0.62 (P = 0.016) for the prospective analysis. Cronbach alpha was 0.89 (90\% CI, 0.86-0.91).           Conclusions:~           Self-reported effects of previous injury may be one method to efficiently identify athletes who possess elevated injury risk, and subsequently deliver preventive interventions, thereby providing an alternative method to time-intensive functional testing.},
  langid = {american},
  file = {/home/david/Zotero/storage/UACGIY38/a_sport_fitness_index_for_assessment_of.12.html}
}

@article{wilsonGoodEnoughPractices2017,
  title = {Good Enough Practices in Scientific Computing},
  author = {Wilson, Greg and Bryan, Jennifer and Cranston, Karen and Kitzes, Justin and Nederbragt, Lex and Teal, Tracy K.},
  date = {2017-06-22},
  journaltitle = {PLoS Computational Biology},
  volume = {13},
  number = {6},
  pages = {1--20},
  publisher = {Public Library of Science},
  issn = {1553734X},
  doi = {10.1371/journal.pcbi.1005510},
  url = {https://ezproxy.massey.ac.nz/login?url=https://search.ebscohost.com/login.aspx?direct=true&AuthType=sso&db=aph&AN=123731587&site=eds-live&scope=site&authtype=sso&custid=s3027306},
  urldate = {2024-07-20},
  abstract = {Author summary:  Computers are now essential in all branches of science, but most researchers are never taught the equivalent of basic lab skills for research computing. As a result, data can get lost, analyses can take much longer than necessary, and researchers are limited in how effectively they can work with software and data. Computing workflows need to follow the same practices as lab projects and notebooks, with organized data, documented steps, and the project structured for reproducibility, but researchers new to computing often don't know where to start. This paper presents a set of good computing practices that every researcher can adopt, regardless of their current level of computational skill. These practices, which encompass data management, programming, collaborating with colleagues, organizing projects, tracking work, and writing manuscripts, are drawn from a wide variety of published sources from our daily lives and from our work with volunteer organizations that have delivered workshops to over 11,000 people since 2010.},
  keywords = {Computer and information sciences,Computer software,COMPUTERS,Control engineering,Control systems,Data management,Data processing,EDUCATION conferences,Engineering and technology,Information technology,MANUSCRIPTS,Mathematics,Perspective,Physical sciences,Programming languages,Reproducibility,RESEARCH,Research and analysis methods,Research assessment,SCIENTIFIC computing,Software engineering,Software tools,Source code,Systems science},
  file = {/home/david/Zotero/storage/J4D9WTCQ/Wilson et al. - 2017 - Good enough practices in scientific computing.pdf}
}

@article{wingelaar-jagtFatigueAviationSafety2021,
  title = {Fatigue in {{Aviation}}: {{Safety Risks}}, {{Preventive Strategies}} and {{Pharmacological Interventions}}},
  shorttitle = {Fatigue in {{Aviation}}},
  author = {Wingelaar-Jagt, Yara Q. and Wingelaar, Thijs T. and Riedel, Wim J. and Ramaekers, Johannes G.},
