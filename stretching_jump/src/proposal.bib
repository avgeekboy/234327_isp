@article{horakovaPulseOximeterPerformance2022,
  title = {Pulse {{Oximeter Performance}} during {{Rapid Desaturation}}},
  author = {Horakova, Lenka and Roubik, Karel},
  date = {2022-06-02},
  journaltitle = {Sensors},
  shortjournal = {Sensors},
  volume = {22},
  number = {11},
  pages = {4236},
  issn = {1424-8220},
  doi = {10.3390/s22114236},
  url = {https://www.mdpi.com/1424-8220/22/11/4236},
  urldate = {2024-07-09},
  abstract = {The reliability of pulse oximetry is crucial, especially in cases of rapid changes in body oxygenation. In order to evaluate the performance of pulse oximeters during rapidly developing short periods of concurrent hypoxemia and hypercapnia, 13 healthy volunteers underwent 3 breathing phases during outdoor experiments (39 phases in total), monitored simultaneously by five different pulse oximeters. A significant incongruity in values displayed by the tested pulse oximeters was observed, even when the accuracy declared by the manufacturers were considered. In 28.2\% of breathing phases, the five used devices did not show any congruent values. The longest uninterrupted congruent period formed 74.4\% of total recorded time. Moreover, the congruent periods were rarely observed during the critical desaturation phase of the experiment. The time difference between the moments when the first and the last pulse oximeter showed the typical study endpoint values of SpO2 85\% and 75\% was 32.1 ± 23.6 s and 24.7 ± 19.3 s, respectively. These results suggest that SpO2 might not be a reliable parameter as a study endpoint, or more importantly as a safety limit in outdoor experiments. In the design of future studies, more parameters and continuous clinical assessment should be included.},
  langid = {english},
  file = {/home/david/Zotero/storage/4HIQYYFJ/Horakova and Roubik - 2022 - Pulse Oximeter Performance during Rapid Desaturati.pdf}
}

