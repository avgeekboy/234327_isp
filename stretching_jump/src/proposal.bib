@article{gumaEmergingRoleNeuregulin2010,
  title = {Emerging Role of Neuregulin as a Modulator of Muscle Metabolism},
  author = {Gumà, A. and Martínez-Redondo, V. and López-Soldado, I. and Cantó, C. and Zorzano, A.},
  date = {2010},
  journaltitle = {American Journal of Physiology - Endocrinology and Metabolism},
  volume = {298},
  number = {4},
  pages = {E742-E750},
  doi = {10.1152/ajpendo.00541.2009},
  url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-77949853820&doi=10.1152%2fajpendo.00541.2009&partnerID=40&md5=b48b0c841079b9963188a29cd97fc51b},
  abstract = {Neuregulin was described initially as a neurotrophic factor involved in the formation of the neuromuscular junction in skeletal muscle. However, in recent years, neuregulin has been reported to be a myokine that exerts relevant effects on myogenesis and the regulation of muscle metabolism. In this new context, the rapid and chronic metabolic effects of neuregulin appear to be related to muscle contraction. Indeed, the effects of neuregulin resemble those of exercise, which are accompanied by an improvement in insulin sensitivity. In this review, we challenge the classical role assigned to neuregulin in muscle and propound the emerging concept of its involvement in the regulation of energetic metabolism and insulin responsiveness. Copyright © 2010 the American Physiological Society.},
  keywords = {ErbB receptors,Exercise,Glucose transport,Insulin,Mitochondrial biogenesis}
}

@inproceedings{guoEvaluationInfluenceSevere2017,
