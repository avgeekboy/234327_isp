@article{prinsCerebralKetoneMetabolism2012,
  title = {Cerebral Ketone Metabolism during Development and Injury},
  author = {Prins, Mayumi L.},
  date = {2012-07},
  journaltitle = {Epilepsy Research},
  volume = {100},
  number = {3},
  pages = {218--223},
  issn = {1872-6844 (Electronic) 0920-1211 (Linking)},
  doi = {10.1016/j.eplepsyres.2011.09.027},
  url = {https://app.dimensions.ai/details/publication/pub.1007595727 http://europepmc.org/articles/pmc3306503?pdf=render https://www.ncbi.nlm.nih.gov/pmc/articles/PMC3306503/pdf/nihms-339800.pdf},
  urldate = {2021-10-08},
  abstract = {Cerebral metabolism of ketones is a normal part of the process of brain development. While the mature brain relies on glucose as a primary fuel source, metabolism of ketone bodies remains an alternative energy source under conditions of starvation. The neuroprotective properties of brain ketone metabolism make this alternative substrate a viable therapeutic option for various pathologies. Since the ability to revert to utilizing ketones as an alternative substrate is greatest in the younger post-weaned brain, this particular therapeutic approach remains an untapped resource particularly for pediatric pathological conditions.},
  keywords = {Aging,Animals,Brain Injuries/drug therapy/*metabolism,Brain/growth & development/*metabolism,Humans,Ketone Bodies/*metabolism,Ketones/*metabolism,Starvation/metabolism}
}

@article{prinsCerebralMetabolicAdaptation2008,
  title = {Cerebral {{Metabolic Adaptation}} and {{Ketone Metabolism}} after {{Brain Injury}}},
  author = {Prins, Mayumi L.},
  date = {2008-01},
  journaltitle = {Cerebrovascular and Brain Metabolism Reviews},
  volume = {28},
  number = {1},
  pages = {1--16},
  issn = {0271-678X (Print) 0271-678X (Linking)},
  doi = {10.1038/sj.jcbfm.9600543},
  url = {https://app.dimensions.ai/details/publication/pub.1027462357 https://journals.sagepub.com/doi/pdf/10.1038/sj.jcbfm.9600543},
  urldate = {2021-10-08},
  abstract = {The developing central nervous system has the capacity to metabolize ketone bodies. It was once accepted that on weaning, the 'post-weaned/adult' brain was limited solely to glucose metabolism. However, increasing evidence from conditions of inadequate glucose availability or increased energy demands has shown that the adult brain is not static in its fuel options. The objective of this review is to summarize the body of literature specifically regarding cerebral ketone metabolism at different ages, under conditions of starvation and after various pathologic conditions. The evidence presented supports the following findings: (1) there is an inverse relationship between age and the brain's capacity for ketone metabolism that continues well after weaning; (2) neuroprotective potentials of ketone administration have been shown for neurodegenerative conditions, epilepsy, hypoxia/ischemia, and traumatic brain injury; and (3) there is an age-related therapeutic potential for ketone as an alternative substrate. The concept of cerebral metabolic adaptation under various physiologic and pathologic conditions is not new, but it has taken the contribution of numerous studies over many years to break the previously accepted dogma of cerebral metabolism. Our emerging understanding of cerebral metabolism is far more complex than could have been imagined. It is clear that in addition to glucose, other substrates must be considered along with fuel interactions, metabolic challenges, and cerebral maturation.},
  keywords = {*Adaptation Physiological,*Energy Metabolism,Adult,Aging,Animals,Brain Injuries/drug therapy/*metabolism,Brain Ischemia/drug therapy/metabolism,Brain/growth & development/*metabolism,Epilepsy/drug therapy/metabolism,Glucose/*metabolism,Humans,Infant,Infant Newborn,Ketone Bodies/*metabolism,Ketones/*metabolism/pharmacology,Mice,Neurodegenerative Diseases/drug therapy/metabolism,Neuroprotective Agents/metabolism/pharmacology,Rats,Weaning},
  file = {/home/david/Zotero/storage/UNH72FVU/Prins - 2008 - Cerebral Metabolic Adaptation and Ketone Metabolis.pdf}
}

@article{puchowiczAdaptationChronicHypoxia2005,
  title = {Adaptation to Chronic Hypoxia during Diet-Induced Ketosis},
  author = {Puchowicz, M. A. and Emancipator, D. S. and Xu, K. and Magness, D. L. and Ndubuizu, O. I. and Lust, W. D. and LaManna, J. C.},
