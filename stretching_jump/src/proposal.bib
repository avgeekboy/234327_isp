  author = {Di Raimondo, D. and Tuttolomondo, A. and Musiari, G. and Schimmenti, C. and D’Angelo, A. and Pinto, A.},
  date = {2016},
  journaltitle = {Current Pharmaceutical Design},
  volume = {22},
