  shorttitle = {Revisión Sistemática de “Miocinas y Regulación Metabólica”},
  author = {León-Ariza, H. H. and Mendoza-Navarrete, M. P. and Maldonado-Arango, M. I. and Botero-Rosas, D. A.},
