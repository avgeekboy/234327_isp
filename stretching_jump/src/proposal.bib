@article{tsangEndocrineRegulationCircadian2016,
  title = {Endocrine Regulation of Circadian Physiology},
  author = {Tsang, A. H. and Astiz, M. and Friedrichs, M. and Oster, H.},
  date = {2016-07},
  journaltitle = {J Endocrinol},
  volume = {230},
  number = {1},
  eprint = {27106109},
